{
  "name": "emoji-extension",
  "version": "1.1.5",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/build.js dev",
    "watch": "node scripts/watch.js",
    "build": "node scripts/build.js build",
    "build:prod": "node scripts/build.js build:prod",
    "build:minimal": "node scripts/build.js build:minimal",
    "build:userscript": "node scripts/build.js build:userscript",
    "build:userscript:oneline": "USERSCRIPT_EMBED_JSON_ONELINE=true node scripts/build.js build:userscript",
    "build:userscript:remote": "node scripts/build.js build:userscript remote",
    "build:userscript:min": "node scripts/build.js build:userscript:min",
    "build:userscript:pc": "USERSCRIPT_PLATFORM=pc node scripts/build.js build:userscript",
    "build:userscript:mobile": "USERSCRIPT_PLATFORM=mobile node scripts/build.js build:userscript",
    "build:userscript:pc:min": "USERSCRIPT_PLATFORM=pc node scripts/build.js build:userscript:min",
    "build:userscript:mobile:min": "USERSCRIPT_PLATFORM=mobile node scripts/build.js build:userscript:min",
    "build:userscript:pc:oneline": "USERSCRIPT_PLATFORM=pc USERSCRIPT_EMBED_JSON_ONELINE=true node scripts/build.js build:userscript",
    "build:userscript:mobile:oneline": "USERSCRIPT_PLATFORM=mobile USERSCRIPT_EMBED_JSON_ONELINE=true node scripts/build.js build:userscript",
    "build:userscript:pc:remote": "USERSCRIPT_PLATFORM=pc node scripts/build.js build:userscript remote",
    "build:userscript:mobile:remote": "USERSCRIPT_PLATFORM=mobile node scripts/build.js build:userscript remote",
    "build:debug": "node scripts/build.js build:debug",
    "serve": "vite preview",
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "test:extension": "playwright test --config=playwright.extension.config.ts",
    "test:extension:debug": "playwright test --config=playwright.extension.config.ts --debug",
    "release": "bash ./scripts/release.sh",
    "pack:crx": "node ./scripts/pack-crx.js",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "ant-design-vue": "^4.2.6",
    "eslint": "^9.36.0",
    "ignore": "^7.0.5",
    "pinia": "^2.3.1",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/typography": "^0.5.19",
    "@types/chrome": "^0.1.12",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.21",
    "crx": "^5.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "jsonc-eslint-parser": "^1.4.1",
    "less": "^4.4.1",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.26.0",
    "vite": "npm:rolldown-vite@^7.1.13",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.0"
  },
  "devDependenciesExtra": {},
  "dependenciesMeta": {},
  "devDependenciesAdditions": {},
  "devDependencies2": {},
  "peerDependenciesMeta": {},
  "bundledDependencies": [],
  "overrides": {},
  "engines": {},
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    }
  }
}
