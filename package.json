{
  "name": "emoji-extension",
  "version": "1.2.9-patch-3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/build.js dev",
    "watch": "node scripts/watch.js",
    "build": "node scripts/build.js build",
    "build:prod": "node scripts/build.js build:prod",
    "build:minimal": "node scripts/build.js build:minimal",
    "build:debug": "node scripts/build.js build:debug",
    "serve": "vite preview",
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "test:extension": "playwright test --config=playwright.extension.config.ts",
    "test:extension:debug": "playwright test --config=playwright.extension.config.ts --debug",
    "release": "bash ./scripts/release.sh",
    "pack:crx": "node ./scripts/pack-crx.js",
    "pack:xpi": "node ./scripts/pack-xpi.js",
    "pack:all": "./scripts/npm-run.sh npm run pack:crx && ./scripts/npm-run.sh npm run pack:xpi && ./scripts/npm-run.sh npm run update:version && ./scripts/npm-run.sh npm run update:data",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "vue-tsc --noEmit",
    "prepare:json": "node scripts/prepare-json-assets.js",
    "update:data": "cd scripts/cfworker && wrangler pages deploy",
    "update:metadata": "node scripts/update-market-metadata.js",
    "update:push": "git push",
    "update:version": "node scripts/update-extension-version.js",
    "update:deploy": "./scripts/deploy-update.sh",
    "update:all": "npm run build:prod && npm run pack:all && npm run update:version && npm run update:deploy",
    "release:all": "node scripts/release-all.js",
    "update:wasm": "bash ./scripts/wasm/build.sh",
    "share": "node ./scripts/collaborative-upload-server/server.js",
    "share:stop": "kill $(lsof -t -i:9527) 2>/dev/null || echo 'Server not running'",
    "check:all": "pnpm lint && pnpm format:check && pnpm type-check"
  },
  "dependencies": {
    "ant-design-vue": "^4.2.6",
    "dexie": "^4.2.1",
    "dompurify": "^3.3.1",
    "ignore": "^7.0.5",
    "libavif-wasm": "^0.1.13",
    "marked": "^16.4.2",
    "nanoid": "^5.1.6",
    "pinia": "^2.3.1",
    "typeit": "^8.8.7",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/chrome": "^0.1.32",
    "@types/dompurify": "^3.2.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^24.10.4",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.23",
    "crx": "^5.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "jsonc-eslint-parser": "^1.4.1",
    "less": "^4.5.1",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.19",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.26.0",
    "vite": "npm:rolldown-vite@^7.3.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.1"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    },
    "onlyBuiltDependencies": [
      "core-js",
      "electron",
      "esbuild",
      "typeit",
      "vue-demi"
    ]
  },
  "bundleDependencies": []
}
